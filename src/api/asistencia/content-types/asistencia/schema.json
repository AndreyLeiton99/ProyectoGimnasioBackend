    },
    "curso": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::curso.curso",
      "inversedBy": "asistencias"
    },
    "date": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "presente",
        "ausente",
        "excluido"
